<!doctype html>
<html class="no-js" lang="">
<head>
  <meta charset="utf-8">
  <title>npm - project statusboard</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css"/>
  <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.10.20/b-1.6.1/b-colvis-1.6.1/b-print-1.6.1/cr-1.5.2/fh-3.1.6/datatables.min.css"/>
  <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,600,700|Source+Sans+Pro&:300,400,600,700Sour&display=swap" rel="stylesheet">
  <style>
    *, *:before, *:after {
      box-sizing: inherit;
    }
    body {
      box-sizing: border-box;
      padding: 0;
      margin: 0;
      font-family: 'Source Sans Pro', 'Lucida Grande', sans-serif;
    }
    body:before {
      content: '';
      display: block;
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 10px;
      color: rgb(65, 56, 68);
      background-image: linear-gradient(139deg, #fb8817, #ff4b01, #c12127, #e02aff);
    }
    main {
      padding: 30px 40px 50px;
    }
    input[type="search"] {
      padding: 10px;
      width: 250px !important;
    }
    h1, h2, h3, h4, h5, h6 {
      font-family: 'Poppins', sans-serif;
    }
    header {
      padding: 25px 40px 15px;
      border-bottom: 1px solid rgba(134, 131, 131, 0.2);
    }
    a,
    a:active,
    a:hover {
      text-decoration: none;
      font-weight: bold;
      font-size: 16px;
      line-height: 100%;
      color: rgb(65, 56, 68); 
    }
    header a {
      font-family: 'Poppins', sans-serif;
      font-weight: 600;
    }
    header a span {
      margin: 0 15px 0 0;
    }
    table tr th:not(:nth-of-type(1)),
    table tr td:not(:nth-of-type(1)) {
      text-align: center;
    }
    table tr td span {
      background: #333;
      font-size: 12px;
      color: white; 
      padding: 5px 0;
      border-radius: 3px;
      width: 100%;
      display: block;
    }
    table tr td {
      vertical-align: middle !important;
    }
    table tr td span.error,
    table tr td span.failure,
    table tr td span.low {
      background: red;
    }
    table tr td span.medium {
      background: orange;
    }
    table tr td span.pending {
      background: blue;
    }
    table tr td span.success,
    table tr td span.high {
      background: green;
    }
    .license {
      text-transform: uppercase;
      font-weight: bold;
    }
    .status {
      text-transform: capitalize;
    }
  </style>
</head>
<body>
  <header>
    <a href="https://npmjs.com/"><span>‚ù§</span>npmjs.com</a>
  </header>
  <main>
    <h2><strong>npm</strong> project statusboard</h2>
    <small>Last built: {{built}}</small>
    <table class="table table-striped table-bordered" data-sortable>
      <thead>
      <tr>
        <th>
          Name
        </th>
        <th>
          Stars
        </th>
        <th>
          Issues
        </th>
        <th>
          Pull&nbsp;Requests
        </th>
        <th>
          License
        </th>
        <th>
          Last Commit
        </th>
        <th>
          Build&nbsp;Status
        </th>
        <th>
          Node
        </th>
        <th>
          Downloads&nbsp;(/m)
        </th>
        <th>
          Dependencies
        </th>
        <th>
          Test&nbsp;Coverage
        </th>
        <th>
          Size&nbsp;(KB)
        </th>
        <th>
          Version
        </th>
      </tr>
      </thead>
      <tbody>
    {{#each repos}}
      <tr>
        <td>
          <a href="https://www.github.com/npm/{{name}}" target="_blank">{{name}}</a>
        </td>
        <td>
          {{stargazers_count}}
        </td>
        <td>
          {{issues_count}}
        </td>
        <td>
          {{prs_count}}
        </td>
        <td class="license">
          {{#if license.key}}
            {{license.key}}
          {{else}}
            <span class="error">N/A</span>
          {{/if}}
        </td>
        <td>
          {{pushed_at}}
        </td>
        <td class="status">
          {{#if build_status}}
            <span class="{{build_status}}">{{build_status}}</span>
          {{/if}}
        </td>
        <td>
          {{node}}
        </td>
        <td>
          {{downloads}}
        </td>
        <td>
          {{#if version}}
            <img alt="dependencies" src="https://img.shields.io/librariesio/release/npm/{{name}}">
          {{/if}}
        </td>
        <td class="coverage">
          {{#if coverage}}
            <span class="{{coverageLevel}}">{{coverage}}</span>
          {{/if}}
        </td>
        <td>
          {{size}}
        </td>
        <td>
          {{version}}
        </td>
      </tr>
    {{/each}}
    </tbody>
    </table>
  </main>
<script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
{{!-- <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script> --}}
<script type="text/javascript" src="https://cdn.datatables.net/v/dt/dt-1.10.20/b-1.6.1/b-colvis-1.6.1/b-print-1.6.1/cr-1.5.2/fh-3.1.6/datatables.min.js"></script>
<script>
$(document).ready(function () {
  $('table').DataTable({
    paging: false,
    colReorder: true,
    buttons: [ 'colvis' ]
  })
})
</script>
</body>
</html>